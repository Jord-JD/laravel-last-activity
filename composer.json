{
  "name": "jord-jd/laravel-last-activity",
  "description": "Stores the last activity time of users within your Laravel application",
  "type": "library",
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.6|^6|^7|^8"
  },
  "autoload": {
    "psr-4": {
      "JordJD\\LaravelLastActivity\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "JordJD\\LaravelLastActivity\\ServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "replace": {
    "divineomega/laravel-last-activity": "self.version"
  }
}
